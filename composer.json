{
    "name": "vormkracht10/oauth2-genesys",
    "description": "This package provides Genesys OAuth 2.0 support for the PHP League's OAauth 2.0 Client",
    "keywords": [
        "vormkracht10",
        "oauth2-genesys"
    ],
    "homepage": "https://github.com/vormkracht10/oauth2-genesys",
    "license": "MIT",
    "authors": [
        {
            "name": "Bas van Dinther",
            "email": "bas@vormkracht10.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "league/oauth2-client": "^2.6",
        "league/oauth2-server": "^8.3",
        "mockery/mockery": "^1.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Vormkracht10\\OAuth2Genesys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vormkracht10\\OAuth2Genesys\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
